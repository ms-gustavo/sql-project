<h1>Usu√°rios Cadastrados:</h1>
<div class="container">
  {{#each users}}
    <div class="item">
      <h2>{{this.name}}</h2>
      <p>{{this.email}}</p>
      <p>{{this.phone}}</p>
      <div class="actions">
        <a href="/users/details/{{this.id}}">Ver detalhes</a>
        <a href="/users/edit/{{this.id}}" class="edit-btn">Editar</a>
        <form action="/users/delete/{{this.id}}" method="POST">
          <input type="submit" value="Excluir" class="delete-btn" /></form>
      </div>
    </div>
  {{/each}}
</div>